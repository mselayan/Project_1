    "# Let us discuss from previous lab how can I group together multiple outputs of statistics\n",
    "\n",
    "# We had this done from previous lab \n",
    "\n",
    "stat_summary = air_data.groupby(['sensor.name']).agg(max=('pm2.5_atm', 'max'),\n",
    "                                                       min=('pm2.5_atm', 'min'),\n",
    "                                                       mean=('pm2.5_atm', 'mean'),\n",
    "                                                       median=('pm2.5_atm', 'median'))\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# If I want to run statistics for multiple groups together\n",
    "\n",
    "stat_summary2 = (\n",
    "    air_data\n",
    "    .groupby('sensor.name')\n",
    "    .agg({\n",
    "        'pm2.5_atm': ['max', 'min', 'mean', 'median'],\n",
    "        'pm1.0_atm':  ['max', 'min', 'mean', 'median'],   # <-- your PM10 column name here\n",
    "    })\n",
    ")\n",
    "\n",
    "\n",
    "# Pivot table for multiple groups \n",
    "\n",
    "\n",
    "pivot_stats = pd.pivot_table(\n",
    "    air_data,\n",
    "    index=\"sensor.name\",\n",
    "    values=[\"pm2.5_atm\", \"pm1.0_atm\"],   # change pm10 column name if different\n",
    "    aggfunc=[\"max\", \"min\", \"mean\", \"median\"]\n",
    ")\n",
    "\n",
    "\n",
    "pivot_stats\n"
   ]
  },
